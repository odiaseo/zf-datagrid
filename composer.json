{
    "name": "synergy/synergydatagrid",
    "description": "Module to display data in grid using the jqGrid javascript plugin",
    "type": "synergy-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "data grid",
        "jqgrid",
        "zf2",
        "module"
    ],
    "homepage": "http://developer.peleodiase.com/synergy-data-grid",
    "authors": [
        {
            "name": "Pele Odiase",
            "email": "peleodiase@yahoo.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "synergy/common": "^8.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-0": {
            "SynergyDataGridTest": "tests/module"
        },
        "psr-4": {
            "SynergyDataGrid\\": "src/SynergyDataGrid"
        },
        "classmap": [
            "./Module.php",
            "tests/module/SynergyDataGridTest/Module.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-component-installer": true,
            "laminas/laminas-dependency-plugin": true
        }
    }
}
