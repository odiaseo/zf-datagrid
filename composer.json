{
    "name": "synergy/synergydatagrid",
    "description": "Module to display data in grid using the jqGrid javascript plugin",
    "type": "synergy-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "data grid",
        "jqgrid",
        "zf2",
        "module"
    ],
    "homepage": "http://developer.peleodiase.com/synergy-data-grid",
    "authors": [
        {
            "name": "Pele Odiase",
            "email": "peleodiase@yahoo.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "laminas/laminas-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zendframework": "^3",
        "doctrine/doctrine-orm-module": "^1.1",
        "laminas/laminas-mvc": "^3",
        "monolog/monolog": "^1",
        "synergy/common": "^7.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "^2.0",
        "phpunit/phpunit": "~6.0"
    },
    "suggest": {
        "gedmo/doctrine-extensions": ">=v2.2.0"
    },
    "autoload": {
        "psr-0": {
            "SynergyDataGridTest": "tests/module"
        },
        "psr-4": {
            "SynergyDataGrid\\": "src/SynergyDataGrid"
        },
        "classmap": [
            "./Module.php",
            "tests/module/SynergyDataGridTest/Module.php"
        ]
    }
}
